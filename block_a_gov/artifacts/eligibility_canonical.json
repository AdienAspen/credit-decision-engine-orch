  "policy": {
    "credit_score_used_in_eligibility": false,
    "early_cut_enabled": true,
    "case_b_review_short_circuit": true
  },
  "notes": {
    "input_contract_doc": "block_a_gov/spec/application_intake_v0_1.md",
    "output_contract_doc": "block_a_gov/spec/eligibility_agent_status_v0_1.md",
